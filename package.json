{
  "name": "vue3-starter",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "type": "vue-tsc --noEmit",
    "start": "node ./app.js",
    "preview": "vite preview",
    "commit": "git pull && pnpm lint:fix && git add -A && pnpm cz && git push",
    "pre-commit": "git pull && pnpm lint-staged && pnpm cz && git push",
    "cz": "czg",
    "prepare": "husky install"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@formkit/auto-animate": "1.0.0-beta.6",
    "@morev/vue-transitions": "^2.3.6",
    "@unocss/reset": "^0.52.4",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "@vueuse/motion": "^2.0.0",
    "axios": "^1.4.0",
    "colord": "^2.9.3",
    "echarts": "^5.4.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vant": "^4.4.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.1",
    "vuetify": "^3.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.3",
    "@eiog/eslint-config": "^0.0.1",
    "@eiog/ts-config": "^0.0.3",
    "@iconify-json/ri": "^1.1.9",
    "@iconify-json/twemoji": "^1.1.11",
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@plugin-web-update-notification/vite": "^1.5.4",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/ui": "^0.31.1",
    "@vue-macros/volar": "^0.10.2",
    "@vue/test-utils": "^2.3.2",
    "commitlint": "^17.6.5",
    "crypto-js": "^4.1.1",
    "cz-git": "^1.6.1",
    "czg": "^1.6.1",
    "eslint": "^8.41.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "naive-ui": "^2.34.4",
    "postcss-preset-env": "^8.4.1",
    "sass": "^1.62.1",
    "taze": "^0.10.1",
    "typescript": "^5.0.4",
    "unocss": "^0.52.4",
    "unocss-preset-extra": "^0.5.2",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.16.2",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.7",
    "unplugin-vue-inspector": "^0.2.5",
    "unplugin-vue-router": "^0.6.4",
    "vfonts": "^0.0.3",
    "vite": "^4.3.9",
    "vite-plugin-pages": "^0.30.1",
    "vite-plugin-pwa": "^0.15.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-component-preview": "^1.1.6",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vue-markdown": "^0.23.5",
    "vite-plugin-vuetify": "^1.0.2",
    "vite-plugin-webfont-dl": "^3.7.4",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.6.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "git add"
    ]
  }
}
